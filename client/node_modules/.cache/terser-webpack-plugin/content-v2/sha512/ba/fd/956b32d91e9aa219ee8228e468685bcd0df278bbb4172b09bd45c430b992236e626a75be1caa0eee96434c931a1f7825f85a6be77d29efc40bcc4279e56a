ippy>\n  );\n}\n\nexport default Note;\n","import React, { useState } from \"react\";\nimport AddIcon from \"@material-ui/icons/Add\";\nimport Fab from \"@material-ui/core/Fab\";\nimport Zoom from \"@material-ui/core/Zoom\";\nimport axios from \"axios\";\n\nfunction get_data(category){\n  var satvik={}\n  axios.get('/'+category+'/display')\n      .then(res=>console.log(res.data))\n      // .then(res=>(res.data))\n      .catch(error=>console.log(error))\n  // console.log(\"in get_data fuction:\" +satvik)\n   return satvik\n}\n\nfunction CreateArea(props) {\n  console.log(\"entered create area\")\n  console.log(props.type)\n  var display_data=get_data('banquet')\n  console.log(display_data.data)\n  const [isExpanded, setExpanded] = useState(false);\n  const [note, setNote] = useState({\n    title: \"\",\n    tel: \"\",\n    type: \"\",\n    cost: \"\", \n    content: \"\"\n  });\n\n  function handleChange(event) {\n    const { name, value } = event.target;\n\n    setNote((prevNote) => {\n      return {\n        ...prevNote,\n        [name]: value\n      };\n    });\n  }\n\n  function submitNote(event) {\n    props.onAdd(note);\n    event.preventDefault();\n  }\n\n  function expand() {\n    setExpanded((prevState) => {\n      return !prevState;\n    });\n  }\n\n  return (\n    <div >\n      <form className=\"create-note\">\n        <div styles={{fontFamily: \"Montserrat\"}}>\n          <h1 onClick={expand} style={{ textAlign: \"center\",fontFamily: \"Montserrat,sans serif\" }}>\n            {\" \"}\n            Welcome!!\n            {\" \"}\n          </h1>\n        </div>\n        {isExpanded && (\n          <h5 style={({ textAlign: \"center\",fontFamily: \"Montserrat,sans serif\" } )}>Edit your details below.</h5>\n        )}\n        {isExpanded && (\n          <h5 style={{ textAlign: \"center\",color:\"white\",fontFamily: \"Montserrat,sans serif\" }}>Edit your details below.</h5>\n        )}\n        {isExpanded && (\n          <input\n            autoComplete=\"off\"\n            name=\"title\"\n            onChange={handleChange}\n            value={note.title}\n            placeholder=\"Your company name\"\n          />\n        )}\n        {isExpanded && (\n          <input\n            autoComplete=\"off\"\n            name=\"tel\"\n            onChange={handleChange}\n            value={note.tel}\n            placeholder=\"Enter your contact...\"\n          />\n        )}\n        {isExpanded && props.id != \"3\" && (\n          <input\n            autoComplete=\"off\"\n            name=\"type\"\n            onChange={handleChange}\n            value={note.type}\n            placeholder= {props.id == 1 ?\"Cuisines specialised in...\" : (props.id == 2 ? \"Seating capacity...\" : \"\")}\n          />\n        )}\n        {isExpanded && (\n          <input\n            autoComplete=\"off\"\n            name=\"cost\"\n            onChange={handleChange}\n            value={note.cost}\n            placeholder={props.id == 1 ?\"Cost per plate\" : (props.id == 2 ?\"Location\" : \"Per hour charge...\")}\n          />\n        )}\n        {isExpanded && (\n          <textarea\n            name=\"content\"\n            autoComplete=\"off\"\n            onChange={handleChange}\n            value={note.content}\n            placeholder=\"About your service...\"\n            rows={isExpanded ? 3 : 1}\n          />\n        )}\n        <Zoom in={isExpanded}>\n          <Fab onClick={submitNote}>\n            <AddIcon onClick={expand} />\n          </Fab>\n        </Zoom>\n      </form>\n    </div>\n  );\n}\n\nexport default CreateArea;\n","import React, { useState } from \"react\";\r\nimport Note from \"./Note\";\r\nimport CreateArea from \"./CreateArea\";\r\nimport axios from \"axios\";\r\n\r\nfunction Caller(props) {\r\n  const [notes, setNotes] = useState([]);\r\n\r\n  function addNote(newNote) {\r\n    console.log(newNote);\r\n    setNotes((prevNotes) => {\r\n      console.log(prevNotes);\r\n      return [newNote];\r\n    });\r\